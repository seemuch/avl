char[] test(char[] x){
	//if we return an subarray, how to display it?	

	int l = len(x);
	if( l <= 1 )
		return x;
	else
		return x[0:l-1];

}

char[] test2(char x[]){

	int l = len(x);
	if( l <= l)
		return x;
	else
		return test(x[0:l-1]);

}
